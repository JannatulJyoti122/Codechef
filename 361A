#include<bits/stdc++.h>
using namespace std;
#define ll long long int
bool isSet(ll n,ll i){
    if(n&(1<<i)==1) return true;
    else return false;
}
int main(){
    ll tt;
    tt=1;
    //cin>>tt;
    while(tt--){
        ll n,k,x;
        cin>>n>>k>>x;
        ll a[n];
        for(ll i=0;i<n;i++) cin>>a[i];
        for(ll i=0;i<k;i++) cout<<a[i]<<" ";
        cout<<x<< " ";
        for(ll i=k;i<n;i++) cout<<a[i]<<" ";
        cout<<endl;
    }
}
