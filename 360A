#include<bits/stdc++.h>
using namespace std;
int main(){
    string s;
    cin>>s;
    int ind=0;
    for(int i=0;i<3;i++){
        if(s[i]=='R') {
            ind=i;
            break;
        }
    }
    int ind1=0;
    for(int i=0;i<3;i++){
        if(s[i]=='M') {
            ind1=i;
            break;
        }
    }
    if(ind<ind1) cout<<"Yes\n";
    else cout<<"No\n";
}
